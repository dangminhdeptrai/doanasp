@model DA.Models.Nhanhang

@{
    ViewData["Title"] = "Sửa nhãn hàng";
}
<br />

<h1>SỬA</h1>

<h4>Nhãn hàng</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SuaNH">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MaNh" />
            <div class="form-group">
                <label asp-for="Ten" class="control-label"></label>
                <input asp-for="Ten" class="form-control" required/>
                <span asp-validation-for="Ten" class="text-danger"></span>
            </div>
            </br>
            <div class="form-group">
                <input type="submit" value="Lưu" class="btn btn-primary" />
                <a asp-action="XemNhanHang" class="btn btn-info">Quay lại</a>
            </div>
        </form>
    </div>
</div>
@*
<div>
    <a asp-action="Index">Quay lại</a>
</div> *@

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
